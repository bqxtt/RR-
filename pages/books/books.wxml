<!--pages/books/books.wxml-->
<view class="title">选择学习词书</view>
<view class="books">
  <view wx:for="{{books}}" wx:key="id" data-tableName="{{item.wordTableName}}" class="book" bindtap="bookFunc">
    <van-image src="../../../../resource/image/book.png" width="200rpx" height="200rpx"></van-image>
    <text >{{item.wordBookName}}</text>
  </view>
</view>